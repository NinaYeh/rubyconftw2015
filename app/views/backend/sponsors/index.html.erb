<h1>index</h1>
<table class="table table-hover">
      <thead>
        <tr>
          <th>Name</th>
          <th>description</th>
          <th>image</th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>
      <% current_collection.each do |sponsor| %>
        <tr>
          <td><%= sponsor.name %></td>
          <td><%= sponsor.description %></td>
          <td><%= image_tag sponsor.image.small %></td>
          <td><%= link_to 'Show', backend_sponsor_path(sponsor) ,class:'btn btn-success btn-xs'%></td>
          <td><%= link_to 'Edit', edit_backend_sponsor_path(sponsor) ,class:'btn btn-success btn-xs'%></td>
          <td><%= link_to 'Destroy', backend_sponsor_path(sponsor), method: :delete, data: { confirm: 'Are you sure?' ,class:'btn btn-danger btn-xs'} %></td>
        </tr>
       <% end %> 
      </tbody>
</table>